{
  "name": "@rokat/web-toolkit",
  "version": "0.1.1",
  "description": "HTTP fetch helpers for Next.js projects.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --watch --format esm,cjs --dts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nextjs",
    "fetch",
    "react-query",
    "utils"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {},
  "peerDependenciesMeta": {},
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
